unit SearchOutputUnit;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants,
  System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.StdCtrls, GraphSearch;

type
  TForm1 = class(TForm)
    lbResults: TLabel;
    procedure FormShow(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
    SearchInfo: TSearchInfo;
  end;

var
  Form1: TForm1;

implementation

{$R *.dfm}

procedure TForm1.FormShow(Sender: TObject);
begin
  lbResults.Caption := 'Информация о поиске'#13#10;
  lbResults.Caption := lbResults.Caption + 'Число дуг: ' +
    IntToStr(Info.ArcsCount) + #13#10;
  lbResults.Caption := lbResults.Caption + 'Длина: ' +
    IntToStr(Info.Distance) + #13#10;
  lbResults.Caption := lbResults.Caption + 'Число посещений: ' +
    IntToStr(Info.VisitsCount);
end;

end.
